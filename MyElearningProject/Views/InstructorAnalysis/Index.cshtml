
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/InstructorLayout/Index.cshtml";
}
<div class="row page-titles"  style="background-color:aliceblue">
    <div class="col-md-5 col-8 align-self-center" >
        <h3 class="text-themecolor m-b-0 m-t-0">E-Learning Admin Paneli</h3>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="javascript:void(0)">Ana Sayfa</a></li>
            <li class="breadcrumb-item active">@ViewBag.breadcrumb</li>
        </ol>
    </div>
    <div class="col-md-7 col-4 align-self-center">

    </div>
</div>

<div class="row">
    <!-- Column -->
    @Html.Action("InstructorPanelPartial", "InstructorAnalysis")
    <div class="col-lg-8 col-xlg-9 col-md-7">
        <div class="card">
            <!-- Nav tabs -->
            <ul class="nav nav-tabs profile-tab" role="tablist">
                <li class="nav-item"> <a class="nav-link active" data-toggle="tab" href="#home" role="tab">Yorumlar</a> </li>
                <li class="nav-item"> <a class="nav-link" data-toggle="tab" href="#settings" role="tab">Kurs Listesi</a> </li>
            </ul>
            <!-- Tab panes -->
            <div class="tab-content">
                @Html.Action("CommentPartial", "InstructorAnalysis")
                <!--second tab-->
                
                @Html.Action("CourseListByInstructor","InstructorAnalysis")
            </div>
        </div>
    </div>
</div>